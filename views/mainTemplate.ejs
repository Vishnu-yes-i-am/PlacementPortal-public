<body>
    <div class="navbar">
        <div class="container">
            <div class="navHead" id="navHead">
                <img src="/assets/utils/logo.png" width="50px" height="50px"></img>
                <div class="navText d-none d-sm-block"> PLACEMENTS, MNIT JAIPUR</div>
            </div>
            <div>
                <div class="navItems navText">
                    <ul class="navMenu">
                        <li class="navItem"> <a class="navLink" href="/">HOME</a> </li>
                        <li class="navItem"> <a class="navLink" href="/contactus">PEOPLE</a> </li>
                        <li class="navItem"> <a class="navLink" href="https://mnit.ac.in" target="_blank">WEBSITE</a>
                        </li>
                        <!-- <li class="navItem"> <a class="navLink" href="">HALL OF FAME</a></li> -->
                        <% if(auth.isAuth){ %>
                            <li class="navItem"> <a class="navLink" href="/dashboard">
                                    <%= auth.role %>DASHBOARD
                                </a> </li>
                            <li class="navItem"> <a class="navLink" href="/auth/signout">LOGOUT</a> </li>
                            <% } else{ %>
                                <li class="navItem"> <a class="navLink" href="/login_student">LOGIN</a> </li>
                                <% } %>

                    </ul>

                </div>
                <script>
                    const navs = document.querySelectorAll('.navItem');
                    const path = window.location.pathname;
                    navs.forEach((nav => {
                        if (nav.firstElementChild.getAttribute("href") === path) {
                            nav.firstElementChild.classList.toggle("active")
                        }
                    }))
                </script>
                <div class="hamburger">
                    <span class="bar"></span>
                    <span class="bar"></span>
                    <span class="bar"></span>
                </div>
            </div>
        </div>
        <script>
            const logo = document.getElementById("navHead");
            logo.addEventListener("click", () => {
                location.href = "/";
            })
            logo.style.cursor = "pointer"
        </script>
    </div>